{
  "brand": {
    "name": "VUNO",
    "description": "Medical AI dashboards and web applications",
    "use_cases": [
      "Medical dashboards",
      "Diagnostic tools",
      "Marketing site",
      "Hospital workstations"
    ]
  },
  "grid_system": {
    "columns": 12,
    "gutter": {
      "xs": 12,
      "sm": 16,
      "md": 20,
      "lg": 24,
      "xl": 24,
      "2xl": 24
    },
    "margin": {
      "xs": 16,
      "sm": 20,
      "md": 24,
      "lg": 32,
      "xl": 40,
      "2xl": 48
    }
  },
  "breakpoints": {
    "xs": 320,
    "sm": 640,
    "md": 768,
    "lg": 1024,
    "xl": 1280,
    "2xl": 1536
  },
  "container_widths": {
    "xs": {
      "max_width": 320,
      "padding": "0 16px"
    },
    "sm": {
      "max_width": 608,
      "padding": "0 16px"
    },
    "md": {
      "max_width": 728,
      "padding": "0 20px"
    },
    "lg": {
      "max_width": 960,
      "padding": "0 32px"
    },
    "xl": {
      "max_width": 1200,
      "padding": "0 40px"
    },
    "2xl": {
      "max_width": 1440,
      "padding": "0 48px"
    }
  },
  "spacing_rhythm": {
    "base_unit": 4,
    "scale": {
      "xs": 4,
      "sm": 8,
      "md": 12,
      "lg": 16,
      "xl": 20,
      "2xl": 24,
      "3xl": 32,
      "4xl": 40,
      "5xl": 48,
      "6xl": 56,
      "7xl": 64,
      "8xl": 80,
      "9xl": 96,
      "10xl": 120,
      "11xl": 144,
      "12xl": 168
    },
    "vertical_rhythm": {
      "line_height_base": 1.5,
      "line_height_heading": 1.2,
      "line_height_tight": 1.25,
      "paragraph_spacing": 24
    }
  },
  "aspect_ratios": {
    "widescreen": {
      "name": "16:9",
      "ratio": "16 / 9",
      "percentage": 56.25,
      "use_cases": [
        "Medical imaging displays",
        "Diagnostic video feeds",
        "Workstation monitors"
      ]
    },
    "square": {
      "name": "1:1",
      "ratio": "1 / 1",
      "percentage": 100,
      "use_cases": ["Patient thumbnails", "Icon displays", "Lab result cards"]
    },
    "report": {
      "name": "4:3",
      "ratio": "4 / 3",
      "percentage": 75,
      "use_cases": [
        "Medical report previews",
        "Scan thumbnails",
        "Historical data charts"
      ]
    },
    "portrait": {
      "name": "3:4",
      "ratio": "3 / 4",
      "percentage": 133.33,
      "use_cases": [
        "Mobile dashboards",
        "Vertical charts",
        "Patient info panels"
      ]
    },
    "ultra_widescreen": {
      "name": "21:9",
      "ratio": "21 / 9",
      "percentage": 42.86,
      "use_cases": [
        "Multi-monitor dashboards",
        "Continuous data feeds",
        "Extended timelines"
      ]
    }
  },
  "z_index_scale": {
    "base": 0,
    "content": 10,
    "dropdown": 100,
    "sticky": 200,
    "fixed": 250,
    "modal_backdrop": 300,
    "modal": 310,
    "popover": 320,
    "tooltip": 330,
    "toast": 400,
    "critical_alert": 500,
    "emergency_overlay": 1000
  },
  "safe_areas": {
    "description": "Mobile safe areas for notched devices and medical app accessibility",
    "ios": {
      "top_notch": "env(safe-area-inset-top)",
      "left": "env(safe-area-inset-left)",
      "right": "env(safe-area-inset-right)",
      "bottom": "env(safe-area-inset-bottom)"
    },
    "medical_app_padding": {
      "top": 16,
      "bottom": 16,
      "left": 16,
      "right": 16
    },
    "critical_area_clearance": 32
  },
  "dashboard_layouts": {
    "primary_dashboard": {
      "name": "Primary Medical Dashboard",
      "description": "Main layout for patient data and diagnostic results",
      "grid_template": "grid-template-columns: repeat(12, 1fr)",
      "sections": {
        "header": {
          "span": 12,
          "height": 80,
          "z_index": 200,
          "sticky": true
        },
        "sidebar": {
          "span": {
            "xs": 12,
            "md": 3
          },
          "position": "left",
          "collapsible": true
        },
        "main_content": {
          "span": {
            "xs": 12,
            "md": 9
          },
          "min_height": 600
        }
      }
    },
    "two_column_analysis": {
      "name": "Two-Column Analysis Layout",
      "description": "Side-by-side comparison for diagnostic analysis",
      "grid_template": "grid-template-columns: repeat(6, 1fr)",
      "sections": {
        "primary_image": {
          "span": {
            "xs": 6,
            "lg": 3
          },
          "aspect_ratio": "16 / 9"
        },
        "secondary_image": {
          "span": {
            "xs": 6,
            "lg": 3
          },
          "aspect_ratio": "16 / 9"
        },
        "analysis_panel": {
          "span": 6,
          "z_index": 10
        }
      }
    },
    "metrics_grid": {
      "name": "Metrics Grid Layout",
      "description": "Display key performance indicators and patient metrics",
      "grid_template": "grid-template-columns: repeat(auto-fit, minmax(240px, 1fr))",
      "card_span": {
        "xs": 12,
        "sm": 6,
        "md": 4,
        "lg": 3,
        "xl": 2
      },
      "card_height": 120,
      "gap": 16
    },
    "timeline_view": {
      "name": "Timeline View",
      "description": "Historical patient data and diagnostic timeline",
      "grid_template": "grid-template-columns: repeat(12, 1fr)",
      "sections": {
        "timeline": {
          "span": 12,
          "min_height": 400
        }
      }
    },
    "card_grid": {
      "name": "Card Grid Layout",
      "description": "Flexible grid for patient lists, reports, or data cards",
      "grid_template": "grid-template-columns: repeat(auto-fill, minmax(280px, 1fr))",
      "card_configurations": {
        "compact": {
          "height": 160,
          "padding": 16,
          "gap": 12
        },
        "standard": {
          "height": 240,
          "padding": 20,
          "gap": 16
        },
        "detailed": {
          "height": 320,
          "padding": 24,
          "gap": 20
        }
      }
    },
    "workstation_layout": {
      "name": "Multi-Monitor Workstation Layout",
      "description": "Optimized for 21:9 ultrawide displays and multiple monitors",
      "grid_template": "grid-template-columns: repeat(21, 1fr)",
      "sections": {
        "left_panel": {
          "span": 5,
          "position": "sticky",
          "top": 80
        },
        "center_primary": {
          "span": 11,
          "aspect_ratio": "21 / 9"
        },
        "right_panel": {
          "span": 5,
          "position": "sticky",
          "top": 80
        }
      }
    },
    "mobile_dashboard": {
      "name": "Mobile-First Dashboard",
      "description": "Responsive layout optimized for tablet and mobile medical apps",
      "grid_template": "grid-template-columns: repeat(4, 1fr)",
      "sections": {
        "patient_header": {
          "span": 4,
          "height": 120,
          "z_index": 200
        },
        "primary_metric": {
          "span": 4,
          "aspect_ratio": "1 / 1",
          "min_height": 160
        },
        "secondary_metrics": {
          "span": 2,
          "height": 140,
          "responsive": {
            "xs": {
              "span": 4
            },
            "sm": {
              "span": 2
            }
          }
        }
      }
    }
  },
  "responsive_breakpoint_behaviors": {
    "xs_mobile": {
      "breakpoint": 320,
      "grid_columns": 4,
      "sidebar": "hidden",
      "navigation": "hamburger_menu",
      "typography_scale": 0.85,
      "spacing_multiplier": 0.75
    },
    "sm_tablet": {
      "breakpoint": 640,
      "grid_columns": 6,
      "sidebar": "hidden",
      "navigation": "tab_bar",
      "typography_scale": 0.9,
      "spacing_multiplier": 0.85
    },
    "md_small_laptop": {
      "breakpoint": 768,
      "grid_columns": 12,
      "sidebar": "collapsible",
      "navigation": "horizontal",
      "typography_scale": 1,
      "spacing_multiplier": 1
    },
    "lg_desktop": {
      "breakpoint": 1024,
      "grid_columns": 12,
      "sidebar": "visible",
      "navigation": "horizontal",
      "typography_scale": 1.05,
      "spacing_multiplier": 1.1
    },
    "xl_large_desktop": {
      "breakpoint": 1280,
      "grid_columns": 12,
      "sidebar": "visible",
      "navigation": "horizontal",
      "typography_scale": 1.1,
      "spacing_multiplier": 1.15
    },
    "2xl_ultrawide": {
      "breakpoint": 1536,
      "grid_columns": 12,
      "sidebar": "visible",
      "navigation": "horizontal",
      "max_content_width": 1440,
      "typography_scale": 1.15,
      "spacing_multiplier": 1.25
    }
  },
  "medical_specific_considerations": {
    "data_density": {
      "minimal": {
        "padding": 24,
        "gap": 20,
        "card_height": 240,
        "description": "Critical patient info, emergency alerts"
      },
      "moderate": {
        "padding": 16,
        "gap": 16,
        "card_height": 160,
        "description": "Standard dashboard, metrics display"
      },
      "high": {
        "padding": 12,
        "gap": 12,
        "card_height": 120,
        "description": "Data tables, detailed analysis, research view"
      }
    },
    "accessibility": {
      "minimum_tap_target": 48,
      "focus_outline_width": 3,
      "color_contrast_ratio": 4.5,
      "reading_line_length": 80,
      "safe_area_padding": 16
    },
    "critical_alert_layout": {
      "z_index": 500,
      "position": "fixed",
      "top": 80,
      "width": "100%",
      "max_width": "100vw",
      "height": "auto",
      "padding": 20,
      "gap": 16,
      "animation": "slide_down"
    },
    "real_time_data_refresh": {
      "animation_duration": 300,
      "transition_ease": "ease-in-out",
      "skeleton_loader_height": 120
    }
  },
  "layout_utilities": {
    "containers": {
      "full_width": {
        "width": "100%",
        "max_width": "100vw",
        "padding": "var(--margin)"
      },
      "standard": {
        "width": "100%",
        "max_width": "var(--container-width)",
        "margin": "0 auto",
        "padding": "var(--margin)"
      },
      "narrow": {
        "width": "100%",
        "max_width": 800,
        "margin": "0 auto",
        "padding": "var(--margin)"
      }
    },
    "flex_utilities": {
      "flex_center": {
        "display": "flex",
        "align_items": "center",
        "justify_content": "center"
      },
      "flex_between": {
        "display": "flex",
        "align_items": "center",
        "justify_content": "space-between"
      },
      "flex_col_center": {
        "display": "flex",
        "flex_direction": "column",
        "align_items": "center",
        "justify_content": "center"
      }
    },
    "grid_utilities": {
      "auto_grid": {
        "display": "grid",
        "grid_template_columns": "repeat(auto-fit, minmax(240px, 1fr))",
        "gap": "var(--spacing-lg)"
      },
      "equal_columns": {
        "display": "grid",
        "grid_template_columns": "repeat(auto-fit, 1fr)",
        "gap": "var(--spacing-lg)"
      }
    }
  }
}
